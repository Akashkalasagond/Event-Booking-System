    <style>
      body {
        font-family: Arial, sans-serif;
        margin: 0;
        padding: 0;
        display: flex;
        flex-direction: column;
        align-items: center;
        justify-content: center;
        height: 100vh;
        background-color: rgb(243, 242, 241);
      }

      .navbar {
        background-color: rgb(23, 35, 78);
        /* Set background color */
        color: white;
        /* Set font color */
        width: 100%;
        /* Full width */
        padding: 10px 20px;
        /* Add padding */
        display: flex;
        justify-content: space-between;
        /* Space between items */
        position: fixed;
        /* Fix the navbar at the top */
        top: 0;
        /* Position at the top */
        z-index: 999;
        /* Ensure it's on top of other elements */
      }

      .container {
        border: 2px solid #ccc;
        padding: 20px;
        margin: 10px auto;
        /* Center the container horizontally */
        text-align: center;
        width: 800px;
        /* Increased width for all containers */
        height: 800px;
        /* Increased height for all containers */
        display: flex;
        flex-direction: column;
        justify-content: center;
        margin-top: 10px;
      }

      .container img {
        display: block;
        margin: 0 auto;
        width: 200px;
        /* Set fixed width for images */
        height: 200px;
        /* Set fixed height for images */
        object-fit: cover;
        /* Maintain aspect ratio */
      }

      .team-info {
        margin-top: 20px;
      }

      .social-icons {
        display: flex;
        justify-content: center;
        align-items: center;
        margin-top: 10px;
      }

      .social-icons a {
        color: #333;
        text-decoration: none;
        margin: 0 10px;
        font-size: 24px;
      }

      .navbar a {
        color: white;
        /* Set link color to white */
        text-decoration: none;
        /* Remove underline */
      }
    </style>
  </head>

